    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "generate-key": "node scripts/generate-api-key.js",
    "db:reset:test": "npx dotenv-cli -e .env.test -- prisma migrate reset --force --skip-seed",
    "test:run-mocha": "npx dotenv-cli -e .env.test -- mocha 'spec/**/*.spec.js'",
    "test": "npm run db:reset:test && npm run test:run-mocha",
    "test:focus": "npm run db:reset:test && npx dotenv-cli -e .env.test -- mocha 'spec/**/*.spec.js' --grep"
  },
